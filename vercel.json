{
  "version": 2,
  "builds": [
    {
      "src": "api_server.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api_server.py"
    }
  ],
  "functions": {
    "api_server.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "env": {
    "VERCEL": "1"
  }
}
